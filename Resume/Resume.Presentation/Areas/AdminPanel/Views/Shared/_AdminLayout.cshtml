﻿
<!DOCTYPE html>
<html lang="en" dir="rtl">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <title>پنل مدیریت | @ViewData["Title"]</title>

    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="/Admin_Panel/plugins/font-awesome/css/font-awesome.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/Admin_Panel/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    @*<link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">*@

    <!-- bootstrap rtl -->
    <link rel="stylesheet" href="/Admin_Panel/dist/css/bootstrap-rtl.min.css">
    <!-- template rtl version -->
    <link rel="stylesheet" href="/Admin_Panel/dist/css/custom-style.css">

    @RenderSection("Styls", false)

</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand bg-white navbar-light border-bottom">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fa fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/" class="nav-link">خانه</a>
                </li>

            </ul>



            <!-- Right navbar links -->
            <ul class="navbar-nav mr-auto">
                <!-- Messages Dropdown Menu -->

                <li class="nav-item">
                    <a class="nav-link" data-widget="control-sidebar" data-slide="true" href="#">
                        <i class="fa fa-th-large"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/" class="brand-link">
                <img src="/Admin_Panel/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">پنل مدیریت</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <div>

                    @*<partial name="_ProfilePicturePartial" />*@
                    
                    <!-- Sidebar Menu -->
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                            <!-- Add icons to the links using the .nav-icon class
                            with font-awesome or any other icon font library -->
                            <li class="btn btn-danger" style="height:35px; margin-top:1px;">
                                <a href="/AdminPanel">
                                    <p style="margin:auto; color:azure">
                                        داشبورد مدیریت
                                    </p>
                                </a>
                            </li>

                            <li class="nav-item has-treeview">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fa fa-pie-chart"></i>
                                    <p>
                                        مدیریت تحصیلات
                                        <i class="right fa fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a class="nav-link"
                                           asp-area="AdminPanel"
                                           asp-controller="Education"
                                           asp-action="ListOfAnEducations">

                                            <i class="fa fa-circle-o nav-icon"></i>

                                            <p>لیست تحصیلات</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link"
                                           asp-area="AdminPanel"
                                           asp-controller="Education"
                                           asp-action="CreateAnEducation">
                                            <i class="fa fa-circle-o nav-icon"></i>
                                            <p>افزودن سابقه</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>

                            <li class="btn btn-danger" style="height:35px; margin-top:1px;">
                                <a href="/Logout" class="red-link">
                                    <p style="color:white;">خروج  </p>
                                </a>
                            </li>


                        </ul>
                    </nav>
                    <!-- /.sidebar-menu -->
                </div>
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        @RenderBody()
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
        <!-- Main Footer -->

    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="/Admin_Panel/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="/Admin_Panel/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Admin_Panel/dist/js/adminlte.js"></script>

    <!-- OPTIONAL SCRIPTS -->
    <script src="/Admin_Panel/dist/js/demo.js"></script>

    <!-- PAGE PLUGINS -->
    <!-- SparkLine -->
    <script src="/Admin_Panel/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jVectorMap -->
    <script src="/Admin_Panel/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="/Admin_Panel/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="/Admin_Panel/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- ChartJS 1.0.2 -->
    <script src="/Admin_Panel/plugins/chartjs-old/Chart.min.js"></script>

    <!-- PAGE SCRIPTS -->
    <script src="/Admin_Panel/dist/js/pages/dashboard2.js"></script>
    @RenderSection("scripts", false)
</body>
</html>